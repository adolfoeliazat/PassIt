<div class="container">
<%- model_class = ItemRequest -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:item_id) %>:</strong></dt>
  <dd><%= @item_request.item_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @item_request.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fromDate) %>:</strong></dt>
  <dd><%= @item_request.fromDate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:toDate) %>:</strong></dt>
  <dd><%= @item_request.toDate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @item_request.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:price) %>:</strong></dt>
  <dd><%= @item_request.price %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              item_requests_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_item_request_path(@item_request), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              item_request_path(@item_request),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>